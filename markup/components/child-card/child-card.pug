mixin child-card(data)
  a(href="#").child-card&attributes(attributes)
    .child-card__mask
      +icon('mask-card')
    .child-card__header
      span.child-card__range-old 8-14 лет
      h2.child-card__title #{data.title}
    .child-card__img
      img(src="__static__img/content/" + data.src + '.jpg' srcset="__static__img/content/" + data.src + '@2x.jpg 2x' width=271 height=274 alt=data.title loading="lazy")
